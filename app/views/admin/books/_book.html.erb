<tr id="<%= dom_id(book) %>">
  <td><%= book.title %></td>
  <td><%= book.author %></td>
  <td><%= book.pages %></td>
  <td><%= book.publish_date %></td>
  <td><%= book.category.name %></td>
  <td>
    <%= link_to t("delete"), admin_book_path(book), method: :delete,
      remote: true, data: {confirm: t("confirm_delete")} %>
  </td>
 </tr>
