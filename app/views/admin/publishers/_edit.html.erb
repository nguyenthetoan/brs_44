<%= form_for @publisher, url: admin_publisher_path do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control", require: true %>

  <%= f.label :description %>
  <%= f.text_field :description, class: "form-control" %>

  <%= f.submit t("update"), class: "btn btn-warning" %>
<% end %>
