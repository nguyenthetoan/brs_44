wb = xlsx_package.workbook
wb.add_worksheet(name: "Users") do |sheet|
  sheet.add_row ["ID", Settings.excel.name, Settings.excel.email, Settings.excel.confirmed, Settings.excel.role]
  @users.each do |user|
    sheet.add_row [user.id, user.name, user.email, user.confirmed?, user.role]
  end
  sheet.add_row [Settings.excel.total, @users.length]
end
